{
	"archive": {
		"exclude": [
			"!/languages",
			"/assets/**/*.css",
			"/assets/**/*.js",
			"!/assets/**/*.min.css",
			"!/assets/**/*.min.js",
			"CONTRIBUTING.md",
			"CODE_OF_CONDUCT.md",
			"composer.json",
			"composer.lock",
			"package.json",
			"package-lock.json",
			"phpcs.xml.dist",
			"phpstan.neon.dist",
			"README.md",
			"node_modules",
			"vendor",
			".editorconfig",
			".gitattributes",
			".gitignore",
			".github",
			".wordpress-org",
			".eslintrc.js",
			".prettierrc.js",
			".*"
		]
	},
	"authors": [
		{
			"homepage": "https://www.mahdiyazdani.com",
			"name": "Mahdi Yazdani"
		}
	],
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	},
	"description": "Pause your store temporarily with scheduling your vacation dates and displaying a user-friendly notice at the top of your shop page.",
	"extra": {
		"scripts-description": {
			"lint:wpcbf": "Fix coding standards warnings/errors automatically with PHP Code Beautifier",
			"lint:wpcs": "Analyze code against the WordPress coding standards with PHP_CodeSniffer",
			"make-pot": "Create a POT file for the WordPress plugin in the current directory"
		}
	},
	"homepage": "https://wordpress.org/plugins/woo-store-vacation",
	"keywords": [
		"WordPress",
		"Plugin"
	],
	"license": "GPL-3.0",
	"name": "mypreview/woo-store-vacation",
	"require": {
		"php": ">=7.2"
	},
	"require-dev": {
		"dealerdirect/phpcodesniffer-composer-installer": "0.7.2",
		"php-parallel-lint/php-parallel-lint": "1.3.2",
		"phpcompatibility/phpcompatibility-wp": "2.1.3",
		"phpunit/phpunit": "9.5.24",
		"squizlabs/php_codesniffer": "3.7.1",
		"szepeviktor/phpstan-wordpress": "1.1.2",
		"woocommerce/woocommerce-git-hooks": "*",
		"woocommerce/woocommerce-sniffs": "*",
		"wp-cli/i18n-command": "2.4.0",
		"wp-coding-standards/wpcs": "2.3.0"
	},
	"scripts": {
		"analyze": "@php ./vendor/bin/phpstan analyze",
		"lint:php": "@php ./vendor/bin/parallel-lint --exclude .git --exclude vendor --exclude node_modules .",
		"lint:wpcbf": "@php ./vendor/squizlabs/php_codesniffer/bin/phpcbf",
		"lint:wpcs": "@php ./vendor/squizlabs/php_codesniffer/bin/phpcs",
		"make-pot": "wp i18n make-pot . languages/woo-store-vacation.pot"
	},
	"type": "wordpress-plugin"
}
